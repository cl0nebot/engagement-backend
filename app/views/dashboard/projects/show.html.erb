<div class="row" style="margin-top: 2rem">
  <div class="large-6 columns">
    <nav aria-label="You are here:" role="navigation">
      <ul class="breadcrumbs">
        <li><%= link_to "Projects", dashboard_projects_path %></li>
        <li>
          <span class="show-for-sr">Current:
          </span>
          <%= @api_key.display_name %>
        </li>
      </ul>
    </nav>
  </div>
</div>
<div class="row" style="margin-top:2rem" data-equalizer="menu">
  <div class="medium-2 columns">
    <div class="callout" data-equalizer-watch="menu">
      <h6 class="text-center">Name:</h6>
      <h6 class="text-center"><b><%= @api_key.display_name %></b></h6>
    </div>
  </div>
  <div class="medium-10 columns" data-equalizer-watch="menu">
    <div class="row">
      <div class="medium-3 columns">
        <div class="callout panel">
          <h6>Created On:
            <b><%= @api_key.created_at.to_s(:day_and_mo) %></b>
          </h6>
        </div>
      </div>
      <div class="medium-3 columns">
        <div class="callout">
          <h6>Links Tracked:
            <b><%= @aggregate_counts.source_url_ct %></b>
          </h6>
        </div>
      </div>
      <div class="medium-3 columns">
        <div class="callout">
          <h6>First Event:
            <b><%= format_time(@aggregate_counts.first_timestamp) %></b>
          </h6>
        </div>
      </div>
      <div class="medium-3 columns">
        <div class="callout">
          <h6>Latest Event:
            <b><%= format_time(@aggregate_counts.last_timestamp) %></b>
          </h6>
        </div>
      </div>
      <div class="medium-12 columns">
        <div class="callout">
          <div class="timeline">
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<div class="row small-up-1 medium-up-2 large-up-3">
  <div class="column column-block menu-item-wrapper">
    <div class="accessory arrow-accessory-orange">
    </div>
    <div class="callout">
      <div class="media-object">
        <div class="media-object-section middle">
          <i style="color: #f39c12; font-size: 4em" class="icon-light-bulb"></i>
        </div>
        <div class="media-object-section main-section">
          <h4 style="padding:1em" class="text-center"><%= link_to "Content Links", dashboard_event_path(@api_key.uuid), style: 'color: #f39c12' %></h4>
        </div>
      </div>
    </div>
  </div>
  <div class="column column-block menu-item-wrapper">
    <div class="accessory arrow-accessory-lightgreen">
    </div>
    <div class="callout">
      <div class="media-object">
        <div class="media-object-section middle">
          <i style="color: #1abc9c; font-size: 4em" class="icon-mail"></i>
        </div>
        <div class="media-object-section main-section">
          <h4 style="padding:1em" class="text-center"><%= link_to "Notifications", dashboard_notification_path(@api_key.uuid), style: "color:#1abc9c" %></h4>
        </div>
      </div>
    </div>
  </div>
  <div class="column column-block menu-item-wrapper">
    <div class="accessory arrow-accessory-yellow">
    </div>
    <div class="callout">
      <div class="media-object">
        <div class="media-object-section middle">
          <i style="color: #f1c40f; font-size: 4em" class="icon-docs"></i>
        </div>
        <div class="media-object-section main-section">
          <h4 style="padding:1em" class="text-center"><%= link_to "Reports", dashboard_reports_path, style: "color:#f1c40f" %></h4>
        </div>
      </div>
    </div>
  </div>
  <div class="column column-block menu-item-wrapper">
    <div class="accessory arrow-accessory-green">
    </div>
    <div class="callout">
      <div class="media-object">
        <div class="media-object-section middle">
          <i style="color: #16a085; font-size: 4em" class="icon-mail"></i>
        </div>
        <div class="media-object-section main-section">
          <h4 style="padding:1em" class="text-center"><%= link_to "Insights", dashboard_insight_path(@api_key.uuid), style: "color:#16a085" %></h4>
        </div>
      </div>
    </div>
  </div>
  <div class="column column-block menu-item-wrapper">
    <div class="accessory arrow-accessory-orange">
    </div>
    <div class="callout">
      <div class="media-object">
        <div class="media-object-section middle">
          <i style="color: #f39c12; font-size: 4em" class="icon-key"></i>
        </div>
        <div class="media-object-section main-section">
          <h4 style="padding:1em" class="text-center"><%= link_to "Settings", dashboard_settings_path, style: "color:#f39c12" %></h4>
        </div>
      </div>
    </div>
  </div>
  <div class="column column-block menu-item-wrapper">
    <div class="accessory arrow-accessory-lightgreen">
    </div>
    <div class="callout">
      <div class="media-object">
        <div class="media-object-section middle">
          <i style="color: #1abc9c; font-size: 4em" class="icon-help"></i>
        </div>
        <div class="media-object-section main-section">
          <h4 style="padding:1em" class="text-center"><%= link_to "Help", dashboard_tutorials_path %></h4>
        </div>
      </div>
    </div>
  </div>
</div>
