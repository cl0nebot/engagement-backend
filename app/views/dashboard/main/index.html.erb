<div class="large-12 columns">
  <div class="row" style="margin-top: 2rem">
    <div class="large-6 columns">
      <nav aria-label="You are here:" role="navigation">
        <ul class="breadcrumbs">
          <li>
            <span class="show-for-sr">Current: </span> Home
          </li>
        </ul>
      </nav>
    </div>
    <div class="large-6 columns">
      <div class="callout">
      </div>
    </div>
  </div>
  <div class="row">
    <div class="large-12 columns">
      <div class="callout">
        <table>
          <thead>
            <tr>
              <th>Api Keys</th>
              <th>Status</th>
              <th>Actions</th>
            </tr>
          </thead>
          <tbody>
            <% @api_keys.each do |key| %>
              <tr>
                <td><%= key.display_name %></td>
                <td>ACTIVE</td>
                <td><%= link_to "Go to Events", dashboard_event_path(key.uuid) %></td>
              </tr>
            <% end %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>